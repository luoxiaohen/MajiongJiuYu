rDiceOneImage.node.x = 642;\r\n                this.playerDiceOneImage.node.y = 83;\r\n                this.playerDiceTwoImage.node.x = 749;\r\n                this.playerDiceTwoImage.node.y = 83;\r\n            }\r\n        }else if(this.node.x == 1748){\r\n            this.playerDiceOneImage.node.x = -610;\r\n            this.playerDiceOneImage.node.y = 83;\r\n            this.playerDiceTwoImage.node.x = -500;\r\n            this.playerDiceTwoImage.node.y = 83;\r\n        }else{\r\n            this.playerDiceOneImage.node.x = -565;\r\n            this.playerDiceOneImage.node.y = -90;\r\n            this.playerDiceTwoImage.node.x = -460;\r\n            this.playerDiceTwoImage.node.y = -90;\r\n        }\r\n    }\r\n    /**展示是否准备好了*/\r\n    public showIsReady(boo:boolean){\r\n        if(boo){\r\n            this.showReadyPoint();\r\n        }\r\n        this.isReadyImage.enabled = boo;\r\n    }\r\n\r\n    /***展示空座*/\r\n    public showNone(){\r\n        this.noneGroup.active = true;\r\n        this.headGroup.active = false;\r\n    }\r\n\r\n    /**展示有人*/\r\n    public showSit(){\r\n        this.noneGroup.active = false;\r\n        this.headGroup.active = true;\r\n        if(GameInfo.ins.nowGameStatus < PlayStauteEnum.CheckBookMaker){\r\n            if(this.sitInfo && this.sitInfo.onReady){\r\n                this.showIsReady(true)\r\n            }else{\r\n                this.showIsReady(false);\r\n            }\r\n        }\r\n    }\r\n\r\n    /**展示名字*/\r\n    private showName(){\r\n        this.nameLabel.string = this.playerInfo.gpid+\"\";\r\n    }\r\n\r\n    /**展示分*/\r\n    private showFen(nowFen : number , changeFen : number = 0){\r\n        if(changeFen){\r\n\r\n        }else{\r\n            this.countLabel.string = nowFen+\"\";\r\n        }\r\n    }\r\n\r\n    /**展示漂*/\r\n    private showPiao(isPiao : boolean){\r\n        this.piaoImage.enabled = isPiao;\r\n    }\r\n\r\n    /**展示庄*/\r\n    public showZhuang(isZhuang : boolean){\r\n        this.zhuangImage.enabled